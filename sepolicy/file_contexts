#
#############################
# Custom files
/custom(/.*)?		u:object_r:custom_file:s0


#############################
# Data files
#
# GPU device
/dev/mali.* u:object_r:gpu_device:s0
/dev/mtkfb_vsync(/.*)? u:object_r:graphics_device:s0

# Wifi device
/dev/wmtWifi(/.*)? u:object_r:wmtWifi_device:s0

